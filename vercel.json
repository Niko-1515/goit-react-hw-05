{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://example.com/api/:path*"
      }
    ],
    "redirects": [
      {
        "source": "/old-route",
        "destination": "/new-route",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/api/:path*",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
    ]
  }
  